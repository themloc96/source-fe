    items3: {
        id: 3,
        name: 'Credit',
    },
    items4: {
        id: 4,
        name: 'Credit card',
    },
    items5: {
        id: 5,
        name: 'Square',
    },
    items6: {
        id: 6,
        name: 'Stripe',
    },
    items7: {
        id: 7,
        name: 'Bank Account ACH',
    },
    items8: {
        id: 8,
        name: 'AU Pest Recommendations & Agreement',
    },
    items9: {
        id: 9,
        name: 'AU Pre-engagement Agreement (Ver. Jan. 2018)',
    },
    items10: {
