<!-- Sidebar Job Preview -->
<div class="job-preview"> 
    <div class="job-preview-header">
        <a href="#openJobDetails" class="btn-modal btn-open-job hover-grey">
            <%- include ("../../svg/outline/outline-open.svg") %>
            <span>Open Job</span>
        </a>
        <div class="icon-group">
            <div class="states relative has-notify notify-btn">
                <%- include ("../../svg/states-sms.svg") %>
                <span class="budget">6</span>
            </div>
            <div class="col-icon hover-grey">
                <%- include ("../../svg/outline/outline-collapse-2.svg") %>
            </div>
            <div class="col-icon close-btn hover-grey" onclick="closeJobPreview(this)">
                <%- include ("../../svg/outline/outline-close.svg") %>
            </div>
        </div>
    </div>

    <div class="information">
        <div class="dropdown dropdown-jobpreview drop-status drop-modified">
            <div class="dropbtn btn-default btn-job-preview selection confirmed relative">
                Confirmed
