<!-- Modal add Stripe -->
<div id="addStripe" class="modal container-modal modal-mothods">
    <div class="modal__overlay bg-fixed"></div>
    <div class="modal__container form-addcard stripe-additional">
        <div class="header-modal">
            <span>Add a Card in Stripe</span>
            <div class="btn btn-main">Add</div>
        </div>

        <div class="content has-form scrolls">
            <%- include ("../../svg/addons-stripe.svg") %>
            <div class="content-info">
                <div class="rows">
                    <div class="txt relative">
                        Card Number
                    </div>
                    <input class="field-input number-card" type="text" name="" placeholder="0000 0000 0000 0000">
                </div>
                <div class="rows d-flex mt-5">
                    <div class="col col-date first-field">
                        <div class="txt">
                            Month
                        </div>
                        <input class="field-input" type="text" name="" placeholder="MM">
                    </div>
                    <div class="col col-date second-field">
                        <div class="txt">
                            Year
                        </div>
                        <input class="field-input" type="text" name="" placeholder="Year">
                    </div>
                    <div class="col col-cvc">
                        <div class="txt">
                            CVC
                        </div>
                        <input class="field-input" type="text" name="" placeholder="000">
                    </div>
                </div>
            </div>

            <div class="detail-additional">
                <div class="rows">
                    <div class="txt">
                        Cardholder name
                    </div>
                    <input class="field-input" type="text" name="" placeholder="Placeholder">
                </div>
                <div class="rows">
                    <div class="txt">
                        Street
                    </div>
                    <input class="field-input" type="text" name="" placeholder="Placeholder">
                </div>
                <div class="rows row-haft">
                    <div class="col">
                        <div class="txt">
                            City
                        </div>
                        <input class="field-input" type="text" name="" placeholder="Placeholder">
                    </div>
                    <div class="col">
                        <div class="txt">
                            Zip
                        </div>
                        <input class="field-input" type="text" name="" placeholder="Placeholder">
                    </div>
                </div>
                <div class="rows row-haft">
                    <div class="col">
                        <div class="txt">
                            State
                        </div>
                        <input class="field-input" type="text" name="" placeholder="Placeholder">
                    </div>
                    <div class="col">
                        <div class="txt">
                            Country
                        </div>
                        <input class="field-input" type="text" name="" placeholder="Placeholder">
                    </div>
                </div>
            </div>

            <div class="more-option" onclick="toggleAdditionCard(this)">
                Additional Options
                <%- include ("../../svg/outline/outline-chevron-down.svg") %>
            </div>
        </div>

        <div class="footer-modal btn-close">
            <span class="">Cancel</span>
        </div>
    </div>
</div>

